import java.util.Scanner;

public class DxDyLeftRight {
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		String str = sc.next();
        int n = str.length();
        
        int[] dx = {1,  0, -1, 0};
        int[] dy = {0, -1,  0, 1};
        int dirNum = 3;
        
        for(int i = 0; i < n; i++) {
        	String dir = str.substring(i, i+1);
        	
        	if (dirNum == 0) dirNum = 1;
        	else if (dirNum == 1) dirNum = 2;
        	else if (dirNum == 2) dirNum = 3;
        	else dirNum = 0;
        	
        	switch (dir) {
			case "L": {
				dirNum = (dirNum - 1 + 4) % 4;
				int nx = dx[dirNum];
				int ny = dy[dirNum];
				break;
			}
			case "R": {
				dirNum = (dirNum + 1) % 4;
				int nx = dx[dirNum];
				int ny = dx[dirNum];
			}
			default:
				throw new IllegalArgumentException("Unexpected value: " + dir);
			}
        }
        
       
        
    }

}

import java.util.Scanner;

public class DxDyLeftRight {
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		String str = sc.next();
        int n = str.length();
        
        int[] dx = {1,  0, -1, 0};
        int[] dy = {0, -1,  0, 1};
        
        int x = 0; int y = 0;
        int nx, ny;
        
        int dirNum = 3;
        
        for(int i = 0; i < n; i++) {
        	String dir = str.substring(i, i+1);
        	

        	
        	switch (dir) {
			case "L": {
	        	if (dirNum == 0) dirNum = 3;
	        	else if (dirNum == 1) dirNum = 0;
	        	else if (dirNum == 2) dirNum = 1;
	        	else dirNum = 2;
				
				dirNum = (dirNum - 1 + 4) % 4;
				nx = dx[dirNum];
				ny = dy[dirNum];
				break;
			}
			case "R": {
	        	if (dirNum == 0) dirNum = 1;
	        	else if (dirNum == 1) dirNum = 2;
	        	else if (dirNum == 2) dirNum = 3;
	        	else dirNum = 0;
	        	
				dirNum = (dirNum + 1) % 4;
				nx = dx[dirNum];
				ny = dx[dirNum];
			}
			case "F": {
				nx = x + dx[dirNum];
				ny = y + dx[dirNum];
			}
			default:
				throw new IllegalArgumentException("Unexpected value: " + dir);
			}
        }
        
        System.out.println(nx + " " + ny);
        
       
        
    }

}
